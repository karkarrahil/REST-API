{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/dist/(.*)",
      "use": "@vercel/static",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ],
  "routes": [
    { "src": "/users(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/dist/index.html" }
  ]
}
