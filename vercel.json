{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/users(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "api/dist/index.html" }
  ]
}
